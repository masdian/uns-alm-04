Minor <- function(A){
if (nrow(A) != ncol(A)) {
"Bukan Matriks Persegi"
} else {
m <- matrix(NA, ncol = ncol(A), nrow = nrow(A))
for (i in 1:nrow(A)) {
for (j in 1:ncol(A)) {
m[i,j] <- minor(A, i, j)
}
}
m
}
}
Kofaktor <- function(A){
if (nrow(A) != ncol(A)) {
"Bukan Matriks Persegi"
} else {
m <- matrix(NA, ncol = ncol(A), nrow = nrow(A))
for (i in 1:nrow(A)) {
for (j in 1:ncol(A)) {
m[i,j] <- (-1)^(i+j)*minor(A, i, j)
}
}
m
}
}
