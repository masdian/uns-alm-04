tb <- function(A, i, j){
A1 <- A
A1[i,] <- A[j,]
A1[j,] <- A[i,]
A <- A1
A
}
ob <- function(A,i,j,k){
if (MASS::is.fractions(A) == TRUE) {
A[i, ] <- A[i, ] + A[j, ] * k
} else {
A[i, ] <- A[i, ] + A[j, ] * k
A <- MASS::fractions(A) }
A
}
kb <- function(A, i, k){
if (MASS::is.fractions(A) == TRUE) {
A[i, ] <- A[i, ] * k
} else {
A[i, ] <- A[i, ] * k
A <- MASS::fractions(A)}
A
}
